{
  "version": 2,
  "builds": [
    {
      "src": "qr_flask_app/app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "qr_flask_app/app.py"
    }
  ],
  "env": {
    "PYTHONPATH": "qr_flask_app"
  },
  "functions": {
    "qr_flask_app/app.py": {
      "maxDuration": 30
    }
  }
}
